opcode sTrim, SS, S

SLine xin

iIndex strindex SLine, " "

SToken strsub SLine, 0, iIndex
SRemaining strsub SLine, iIndex + 1

xout SToken, SRemaining

endop


SLine = "Yallah? Salah Abdallah!"
SToken, SRemaining sTrim SLine

prints "Line: %s\n", SLine
prints "Token: %s\n", SToken
prints "Remaining: %s\n", SRemaining
